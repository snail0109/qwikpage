    "css": "^3.0.0",
    "dayjs": "^1.11.10",
    "immer": "^10.0.3",
    "lodash-es": "^4.17.21",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "yorkie": "^2.0.0",
    "vite": "^5.0.8"
