{
  "name": "qwikpage",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "preview": "vite preview",
    "tauri": "tauri",
    "dev": "pnpm --filter editor start",
    "build": "rimraf -rf dist/editor && pnpm --filter editor build",
    "start:admin": "pnpm --filter admin start",
    "build:admin": "rimraf -rf dist/admin && pnpm --filter admin build",
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-os": "~2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@ant-design/icons": "^5.2.6",
    "@ant-design/plots": "^1.2.6",
    "@types/qs": "^6.9.15",
    "ahooks": "^3.7.8",
    "antd": "^5.21.1",
    "axios": "^0.27.2",
    "classnames": "^2.5.1",
    "copy-to-clipboard": "^3.3.3",
    "less": "^4.2.0",
    "lodash-es": "^4.17.21",
    "qs": "^6.12.1",
    "react-error-boundary": "^4.0.13",
    "react-json-view": "^1.21.3",
    "react-quill": "^2.0.0",
    "react-responsive": "^10.0.0",
    "react-router-dom": "^6.21.2",
    "rimraf": "^6.0.1",
    "standard-version": "^9.5.0",
    "styled-components": "^6.1.13",
    "vite": "^5.0.8",
    "vite-plugin-externals-new": "^1.5.5",
    "vite-plugin-svgr": "^4.2.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "typescript": "~5.6.2",
    "vite": "^6.0.3",
    "@types/lodash-es": "^4.17.7",
    "cross-env": "^7.0.3",
    "lint-staged": "^10.0.7",
    "only-allow": "^1.2.1",
    "scp2": "^0.5.0",
    "yorkie": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "react-json-view": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
